<script src="https://www.google.com/recaptcha/api.js"></script>

<!-- Style reCAPTCHA -->
<style>
  #recaptcha > div {
  margin: 0 auto .5em;
}
</style>
<!-- End Style reCAPTCHA -->

<!-- Style Buttons -->
<style>
    .btn-blogger button {
        margin: 20px 0px;
        width: 100%;
